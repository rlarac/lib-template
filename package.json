  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.14",
    "cross-env": "^7.0.2",
    "jest": "^26.5.2",
    "jest-css-modules": "^2.1.0",
    "jest-serializer-html": "^7.0.0",
