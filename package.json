  "name": "lib-template",
  "version": "1.0.0",
  "description": "library template with typescript &amp; rollup",
  "main": "dist/lib-template.umd.min.js",
  "types": "dist/index.d.ts",
  "module": "dist/lib-template.es.min.js",
  "files": [
    "dist",
    "README.md",
    "dist/index.d.ts"
  ],
  "scripts": {
